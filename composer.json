{
    "name": "daijulong/laravel-sms",
    "description": "一个灵活的、易于扩展的适用于Laravel5.5+的短信发送工具包",
    "type": "library",
    "require": {
        "php": ">=7.0",
        "ext-curl":"*",
        "daijulong/sms": "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "daijulong",
            "email": "daijulong@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Daijulong\\LaravelSms\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
    	"laravel": {
    		"providers": [
    			"Daijulong\\LaravelSms\\Providers\\SmsServiceProvider"
    		],
    		"aliases": {
              "Sms": "Daijulong\\LaravelSms\\SmsSender"
            }
    	}
    }
}
